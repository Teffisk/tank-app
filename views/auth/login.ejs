<h2>Log In</h2>
<div class="row">
	<form action="/auth/login" method="POST" class="col s12">
		<div class="row">
			<div class="col s12 input-field">
				<label for="email">Email: </label>
				<input type="email" id="emailinput" name="email">
			</div>
		</div>
		<div class="row">
			<div class="col s12 input-field">
				<label for="password">Password: </label>
				<input type="password" id="password" name="password">
			</div>
		</div>
			<div class="row">
				<div class="col s12 input-field">
					<button class="btn waves-effect waves-light submitbtn" type="submit" name="action">Log In
    					<i class="material-icons right">send</i>
					</button>
				</div>
			</div>
		</div>
	</form>
</div>